def remove_vowels(string):
    vowels=['a','e','i','o','u']
    new_string=""

    for char in string:
     if char.lower()not in vowels:
        new_string+=char

     return new_string

input_string=" hey hi i like you"
new_string=remove_vowels(input_string)

print("original string:",input_string)
print("String with vowels removed:",new_string)
